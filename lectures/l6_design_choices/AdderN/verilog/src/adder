    .scope S_0x55c663bee180;
T_0 ;
    %vpi_call 2 19 "$dumpfile", "adder.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars" {0 0 0};
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x55c663bfd6d0_0, 0, 8;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x55c663bfd7c0_0, 0, 8;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_0;
