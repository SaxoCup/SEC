 .timescale -9 -12;
    .port_info 0 /INPUT 8 "data_in1"
    .port_info 1 /INPUT 8 "data_in2"
    .port_info 2 /OUTPUT 8 "data_out"
P_0x55c663bb80a0 .param/l "N" 0 3 4, +C4<00000000000000000000000000001000>;
v0x55c663bb8190_0 .net "data_in1", 7 0, v0x55c663bfd6d0_0;  1 drivers
v0x55c663bfd480_0 .net "data_in2", 7 0, v0x55c663bfd7c0_0;  1 drivers
v0x55c663bfd560_0 .net "data_out", 7 0, L_0x55c663bfd990;  alias, 1 drivers
